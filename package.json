  "description": "An easy-to-use session component with included warning and renewal modals.",
  "main": "./dist/library.umd.js",
  "module": "./dist/library.es.js",
  "exports": {
    ".": {
      "import": "./dist/library.es.js",
      "require": "./dist/library.umd.js"
    }
  },
  "unpkg": "./dist/library.umd.js",
  "jsdelivr": "./dist/library.umd.js",
  "files": [
    "dist"
  ],
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anedomansky/react-session.git"
  },
  "keywords": [
    "react",
    "session",
    "modal"
  ],
  "author": "Alexander Nedomansky <nedomanskya@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anedomansky/react-session/issues"
  },
  "homepage": "https://github.com/anedomansky/react-session#readme",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.8.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.25.1",
    "@vitest/ui": "^0.25.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "jsdom": "^20.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.25.1"
  }
